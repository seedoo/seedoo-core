<openerp>
    <data noupdate="0">

        <!-- Sequences for protocollo.protocollo -->
        <record id="seq_type_protocollo" model="ir.sequence.type">
            <field name="name">Protocollo Default Sequence Type</field>
            <field name="code">protocollo.sequence.def</field>
        </record>
        <record id="seq_def_protocollo" model="ir.sequence">
            <field name="name">Protocollo Default Sequence</field>
            <field name="code">protocollo.sequence.def</field>
            <field name="implementation">no_gap</field>
            <field name="prefix"></field>
            <field name="suffix"></field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
