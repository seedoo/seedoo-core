<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_sd_protocollo_registro_tree" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.tree</field>
            <field name="model">sd.protocollo.registro</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='sequence_id']" position="after">
                    <field name="aoo_id"
                           required="1"
                    />
                </xpath>

            </field>
        </record>

        <record id="view_sd_protocollo_registro_form" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.form</field>
            <field name="model">sd.protocollo.registro</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="aoo_id"
                           options="{'no_open': True, 'no_create': True}"
                    />
                </xpath>

            </field>
        </record>

        <record id="view_sd_protocollo_registro_search" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.search</field>
            <field name="model">sd.protocollo.registro</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_search"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='codice']" position="after">
                    <field name="aoo_id"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>