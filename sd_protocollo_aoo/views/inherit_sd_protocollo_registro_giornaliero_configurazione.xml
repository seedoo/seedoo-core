<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="view_sd_protocollo_registro_giornaliero_configurazione_tree" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.giornaliero.configurazione.tree</field>
            <field name="model">sd.protocollo.registro.giornaliero.configurazione</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_giornaliero_configurazione_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='nome']" position="after">
                        <field name="aoo_id"/>
                </xpath>

            </field>
        </record>

        <record id="view_sd_protocollo_registro_giornaliero_configurazione_form" model="ir.ui.view">
            <field name="name">view.sd.protocollo.registro.giornaliero.configurazione.form</field>
            <field name="model">sd.protocollo.registro.giornaliero.configurazione</field>
            <field name="inherit_id" ref="sd_protocollo.view_sd_protocollo_registro_giornaliero_configurazione_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='company_id']" position="before">
                        <field name="aoo_id"
                               options="{'no_open': True, 'no_create': True}"
                        />
                </xpath>

            </field>
        </record>

    </data>
</odoo>